
  <h3>Create a New Article</h3>
  <%= form_for @article do |f| %>
  <p>
    <%= f.label :author %>
    <%= f.text_field :author %>
  </p>
  <p>
    <%= f.label :title  %>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :body%>
    <%= f.text_area :body%>
  </p>
  <p>
    <%= f.label :image_url%>
    <%= f.text_area :image_url%>
  </p>
  <p>
    <%= f.submit %>
  </p>
  <% end %>
